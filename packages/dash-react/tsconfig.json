{
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.dash.tsbuildinfo",

    "baseUrl": ".",
    "paths": {
      "~/*": ["./src/*"]
    },

    "target": "ES2020",
    "module": "ESNext",
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "types": ["vite/client"],

    "moduleDetection": "force",
    "moduleResolution": "bundler",

    "jsx": "react-jsx",

    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "useDefineForClassFields": true
  }
}
