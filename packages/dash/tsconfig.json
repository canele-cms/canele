{
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.dash.tsbuildinfo",

    "target": "ES2020",
    "module": "ESNext",
    "lib": ["ES2020", "DOM", "DOM.Iterable"],

    "moduleDetection": "force",
    "moduleResolution": "bundler",

    "jsx": "react-jsx",
    "jsxImportSource": "preact",

    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "useDefineForClassFields": true
  }
}
