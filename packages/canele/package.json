{
  "name": "canele",
  "type": "module",
  "scripts": {
    "dev": "node src/dev.ts",
    "test": "vitest",
    "build": "rm -rf dist && pkgroll"
  },
  "dependencies": {
    "vite": "5.3.5",
    "vite-node": "2.0.3"
  },
  "devDependencies": {
    "@types/node": "20.14.12",
    "pkgroll": "2.1.1",
    "typescript": "5.5.4",
    "vitest": "2.0.3"
  },
  "files": [
    "dist",
    "bin"
  ],
  "bin": {
    "canele-dev": "dist/bin/dev.js"
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./jsx-runtime": {
      "types": "./dist/jsx-runtime.d.ts",
      "import": "./dist/jsx-runtime.mjs",
      "require": "./dist/jsx-runtime.cjs"
    },
    "./jsx-dev-runtime": {
      "types": "./dist/jsx-runtime.d.ts",
      "import": "./dist/jsx-runtime.js",
      "require": "./dist/jsx-runtime.cjs"
    }
  }
}
