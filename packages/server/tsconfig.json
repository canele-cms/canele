{
  "compilerOptions": {
    "outDir": "dist",

    "target": "ESNext",
    "module": "NodeNext",

    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",

    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "verbatimModuleSyntax": true,
    "noUncheckedIndexedAccess": true,
    "noFallthroughCasesInSwitch": true
  }
}
